background yes
use_xft yes
xftfont 123:size=8
xftalpha 0.1
update_interval 1
total_run_times 0
own_window yes
own_window_type desktop
own_window_colour 1a1a1a
own_window_transparent no
own_window_hints undecorated,below,sticky,skip_taskbar,skip_pager
double_buffer yes
minimum_size 250 1051
maximum_width 249
draw_shades no
draw_outline no
draw_borders yes
draw_graph_borders no
border_width 1
border_inner_margin 7
border_outer_margin 7
default_color b58900
default_shade_color red
default_outline_color green
alignment top_right
gap_x 15
gap_y 14
no_buffers no
uppercase no
cpu_avg_samples 2
net_avg_samples 1
override_utf8_locale yes
use_spacer yes
text_buffer_size 256

